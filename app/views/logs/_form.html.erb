<%= form_for @log, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :message, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :message, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :price_found, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :price_found, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :name_found, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :name_found, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :type, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :type, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :ok, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :ok, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :ok_all, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :ok_all, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :url_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :url_id, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                logs_path, :class => 'btn' %>
  </div>
<% end %>
